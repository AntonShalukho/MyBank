    "start": "craco start --config ./craco.config.js",
    "sliced": "cross-env NODE_ENV=sliced npm start",
    "start:development": "craco start --config ./craco.config.js",
    "start:test": "craco start --config ./craco.config.release.js",
    "start:production": "craco start --config ./craco.config.main.js",
    "build": "craco build --config ./craco.config.js",
    "build:development": "craco build --config ./craco.config.js",
    "build:test": "craco build --config ./craco.config.release.js",
    "build:production": "craco build --config ./craco.config.main.js",
    "test": "craco test",
    "test:sliced": "craco test ./src/src",
